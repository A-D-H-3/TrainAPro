            "displayValue": ":root {\n    --color: #880074;\n  }\n\n.button-new {\n\tcolor: #880074;\n\t//color: #1E0A25;\n  background: white;\n}\n  \n  .content {\n    padding: 0px;\n    max-width: 600px;\n    margin: 0 auto;\n    z-index:1;\n    /*padding: 0 20px;*/\n  }\n  \n  .hero {\n    //margin-top:-5px;\n    padding-top: 0px;\n    position: relative;\n    background: #ff142c;\n    color: white;\n    height: 94vh;\n    display: flex;\n    align-items: center;\n    overflow: hidden;\n    padding-bottom: 0px;\n    margin-bottom: 0px;\n    bottom: 0px;\n  }\n  \n  .hero h2 {\n    position: relative;\n    z-index: 2;\n    font-size: 7rem;\n    margin: 0 0 10px;\n    line-height: 1;\n    color: rgba(255, 255, 255, 0.9);\n  }\n  \n  .hero p {\n    position: relative;\n    z-index: 2;\n    font-size: 3rem;\n    color: rgba(255, 255, 255, 0.5);\n    line-height: 1.4;\n  }\n  \n  /* ========================= */\n  \n  .waves {\n    position: absolute;\n    bottom: 0;\n    left: 0;\n    right: 0;\n    height: 150px;\n    background-color: var(--color);\n    transition: 500ms;\n  }\n  \n  .waves::before,\n  .waves::after {\n    content: '';\n    position: absolute;\n    width: 300vw;\n    height: 300vw;\n    top: -70vw;\n    left: 50%;\n    transform: translate(-50%, -75%);\n  }\n  \n  .waves::before {\n    border-radius: 80vh;\n    background: #880074;\n    animation: waves 10s linear infinite;\n  }\n  \n  .waves::after {\n    border-radius: 80vh;\n    background: rgba(51, 51, 51, 0.5);\n    animation: waves 16s linear infinite;\n  }\n  \n  @keyframes waves {\n    0% {\n      transform: translate(-50%, -75%) rotate(0deg);\n    }\n    \n    100% {\n      transform: translate(-50%, -75%) rotate(360deg);\n    }\n  }\n  ",
